{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/500\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/blog\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/contact-us\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/cookies\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/privacy\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/robots\\.txt\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/services\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/work\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}